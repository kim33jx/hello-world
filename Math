var = x,y,z;
document.write ("x = 2"); // prints a message
document.write (x); // prints the value of x
document.write (", y = 56"); // prints a message
document.write (y); // prints value of y
document.write ("<br>x + y is "); // prints a string message
var = a,b,c;
document.write ("a = 3"); // prints a message
document.write (a); // prints the value of a
document.write (", b = 6"); // prints a message
document.write (b); // prints value of b
document.write ("<br>a * c is "); // prints a message
c = a * b; // multiplies x * y and assigns result to z 	 		
